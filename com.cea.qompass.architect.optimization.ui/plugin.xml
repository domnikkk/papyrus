<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
  
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            allPopups="true"
            locationURI="com.cea.qompass.architect.optimization.modelexplorer.popupmenu.generate">
         <command
               commandId="com.cea.qompass.ui.explorer.popupmenu.optimization.GenerateMILP"
               icon="icons/milp.gif"
               id="org.eclipse.robotML.generateRTMapsCommand"
               label="Generate to MILP"
               style="push"
               tooltip="Generate code to RTMaps middleware">
            <!--visibleWhen>
				<reference
                     definitionId="org.eclipse.papyrus.robotml.diagram.common.IsPapyrusActiveWithRobotml">
               </reference>							
			</visibleWhen-->		
         </command>
      </menuContribution>
      <menuContribution
       allPopups="false"
       locationURI="popup:com.cea.qompass.ui.explorer.popupmenu.optimization2">
    <command
          commandId="com.cea.qompass.ui.explorer.popupmenu.optimization.GenerateMILP"
          style="push">
    </command>
 </menuContribution>
 <menuContribution
             allPopups="false"
             locationURI="popup:com.cea.qompass.ui.explorer.popupmenu.main?after=com.cea.qompass.ui.explorer.popupmenu.schedanalysis">
          <menu
                id="com.cea.qompass.ui.explorer.popupmenu.optimization2"
                label="Optimization">
          </menu>
       </menuContribution>
   </extension>
   <extension
         point="org.eclipse.ui.commands">
      <command
            categoryId="org.eclipse.papyrus.editor.category"
            description="Generate MILP data file"
            id="com.cea.qompass.ui.explorer.popupmenu.optimization.GenerateMILP"
            name="GenerateMILP File">
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.handlers">
      <handler
            class="com.cea.qompass.architect.optimization.ui.handler.GenerateOptimizationCommandHandler"
            commandId="com.cea.qompass.ui.explorer.popupmenu.optimization.GenerateMILP">
      </handler>
   </extension>
   
</plugin>

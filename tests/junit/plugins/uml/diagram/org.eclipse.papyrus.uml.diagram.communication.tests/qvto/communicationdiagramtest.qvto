main() {
	topContainerEditPart := "InteractionEditPart";
	topNodesToTest := Set{
		"InteractionEditPart" 
	} ;
	childNodesToTest := Set{
		"LifelineEditPartCN", 
		"ConstraintEditPartCN",
		"CommentEditPartCN" 

import CanonicalTests;

modeltype UML uses uml('http://www.eclipse.org/uml2/5.0.0/UML');

transformation UseCaseDiagramTest(in gmfgenUml : UML, in gmfgenMm : UML, in frameworkBase : UML, in utp : UML, out testModel : UML) extends CanonicalTests(in gmfgenUml : UML, in gmfgenMm : UML, in frameworkBase : UML, in utp : UML, out testModel : UML);

main() {
	diagramUpdater := "CustomUMLDiagramUpdater";
	topContainerEditPart := "PackageEditPartTN";
	topNodesToTest := Set{
		"PackageEditPartTN",
		"ActorEditPartTN",
		"CommentEditPartTN",
		"ConstraintEditPartTN",
		"UseCaseEditPartTN"
	} ;
	childNodesToTest := Set{
		"PackageEditPartCN",
		"ActorInPackageEditPart",
		"CommentEditPartCN",
		"ConstraintInPackageEditPart",
		"ComponentInPackageEditPart",
		"UseCaseInPackageEditPart"
	};
	linksToTest := Set{
		"DependencyEditPart",
		"AssociationEditPart",
		"AbstractionEditPart",
		"UsageEditPart",
		"RealizationEditPart"
	};
	linksOwnedBySourceToTest := Set{
		"GeneralizationEditPart",
		"PackageImportEditPart",
		"PackageMergeEditPart",
		"ExtendEditPart",
		"IncludeEditPart"
	};
	elementTypesAppearanceTest := Set{
		"PackageEditPartTN",
		"ActorEditPartTN",
		"CommentEditPartTN",
		"ConstraintEditPartTN",
		"UseCaseEditPartTN"
	};
	elementTypesDropTests := Set{
		"PackageEditPartTN",
		"ActorEditPartTN",
		"CommentEditPartTN",
		"ConstraintEditPartTN",
		"UseCaseEditPartTN"
	};
	gmfgenUml.rootObjects()[UML::Model]->collectOne(canonicalTests("org.eclipse.papyrus.uml.diagram.usecase.test"));
}

